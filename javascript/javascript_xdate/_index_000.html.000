		function toRoman( year ) {
			//trace( year );
			
			var r = "";
			var y = year;
			
			var i = 0;
			var s = "";
			
			var a1000 = [ "M" , "MM" , "MMM" , "MMMM" ];
			
			var _1000 = parseInt( y / 1000 );			
			if( _1000 > 0 ) {
				/*
				i = _1000;
				while( i-- ) { r += "M" };
				y -= _1000 * 1000;
				*/
				
				r += a1000[ _1000-1 ];
				y -= _1000 * 1000;
			}			
			//trace( r , y );
			
			var _100 = parseInt( y / 100 );			
			if( _100 > 0 ) {
			
				switch( _100 ) {
					case 1: s = "C"; break;
					case 2: s = "CC"; break;
					case 3: s = "CCC"; break;
					case 4: s = "CD"; break;
					case 5: s = "D"; break;
					case 6: s = "DC"; break;
					case 7: s = "DCC"; break;
					case 8: s = "DCCC"; break;
					case 9: s = "CM"; break;
				}
				r += s;
				y -= _100 * 100;
			}
			
			//trace( r , y );
			
			var _10 = parseInt( y / 10 );			
			if( _10 > 0 ) {			
				switch( _10 ) {
					case 1: s = "X"; break;
					case 2: s = "XX"; break;
					case 3: s = "XXX"; break;
					case 4: s = "XL"; break;
					case 5: s = "L"; break;
					case 6: s = "LX"; break;
					case 7: s = "LXX"; break;
					case 8: s = "LXXX"; break;
					case 9: s = "XC"; break;				
				}
				r += s;
				y -= _10 * 10;			
			}
			
			var _1 = parseInt( y / 1 );			
			if( _1 > 0 ) {
				switch( _1 ) {				
					case 1: s = "I"; break;
					case 2: s = "II"; break;
					case 3: s = "III"; break;
					case 4: s = "IV"; break;
					case 5: s = "V"; break;
					case 6: s = "VI"; break;
					case 7: s = "VII"; break;
					case 8: s = "VIII"; break;
					case 9: s = "IX"; break;
				}
				r += s;
				y -= _1 * 1;
			}
			
			trace( r , y );
			
			//trace( _10 );
			
			//trace( parseInt( year / 1000 ) );
		};
		
		function fromRoman( year ) {
			trace( year );
		};
		
